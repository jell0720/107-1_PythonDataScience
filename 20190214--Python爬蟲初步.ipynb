    "import requests, os\n",
    "from bs4 import BeautifulSoup\n",
    "import urllib.request\n",
    "\n",
    "host = 'https://apps.nknush.kh.edu.tw/ZeroBB/'\n",
    "url = ''\n",
    "downloadpath = 'C:\\\\Users\\\\jiangsir\\\\Downloads\\\\ZeroBB\\\\'\n",
    "\n",
    "def download(url, filename):\n",
    "    r = requests.get(url)\n",
    "    filename = os.path.join(downloadpath, filename)\n",
    "    with open(filename,'wb') as f:     #二進位制寫入\n",
    "        f.write(r.content)\n",
    "    \n",
    "\n",
    "for i in range(1, 6):\n",
    "    #print('第 '+str(i)+ '頁 '+ host+url)\n",
    "    result = requests.get(host+url)\n",
    "    soup = BeautifulSoup(result.text, 'lxml')\n",
    "    selector = \"td a\"\n",
    "    tags = soup.select(selector) # 取得一組 bs4.element.Tag 的 list    \n",
    "    for tag in tags:\n",
    "        if 'Download' in tag['href']:\n",
    "            #print(tag.text, host+tag['href'])\n",
    "            i = tag['href'].find('id=')\n",
    "            #print(tag['href'][i+3:])\n",
    "            #print(host+tag['href'], tag['title'])\n",
    "            download(host+tag['href'], tag['href'][i+3:]+'_'+tag['title'])\n",
    "        \n",
    "    url = '?&page='+str(i+1)\n",
    "\n",
    "os.listdir(downloadpath)"
   ]
